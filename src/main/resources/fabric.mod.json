{
	"schemaVersion": 1,
	"id": "no-hat-for-da-ground",
	"version": "${version}",
	"name": "No Hat For Da Ground",
	"description": "Prevents items with custom model data from being placed. Da ground no need yo hat.",
	"authors": [
		"skycatminepokie"
	],
	"contact": {
		"sources": "https://github.com/skycatminepokie/no-hat-for-da-ground"
	},
	"license": "MIT",
	"icon": "assets/no-hat-for-da-ground/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.skycat.nohatfordaground.NoHatForDaGround"
		],
		"client": [
			"com.skycat.nohatfordaground.NoHatForDaGroundClient"
		],
		"fabric-datagen": [
			"com.skycat.nohatfordaground.NoHatForDaGroundDataGenerator"
		]
	},
	"mixins": [
		"no-hat-for-da-ground.mixins.json",
		{
			"config": "no-hat-for-da-ground.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.3",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": "*"
	}
}